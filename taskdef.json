{
  "family": "ecs-simple-node",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::137068240060:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::137068240060:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "ecs-simple-node",
      "image": "137068240060.dkr.ecr.eu-north-1.amazonaws.com/mrrobertamoah/ecs-simple-node:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ecs-simple-node",
          "awslogs-region": "eu-north-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "ENVIRONMENT",
          "value": "production"
        },
        {
          "name": "APP_NAME",
          "value": "ECS Simple Node"
        },
        {
          "name": "APP_VERSION",
          "value": "1.0.0"
        },
        {
          "name": "COLOR",
          "value": "#e6f7ff"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX"
  }
}